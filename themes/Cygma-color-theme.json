  "tokenColors": [
    //#region <General>
    {
      "scope": ["punctuation.definition.comment", "comment"],
      "settings": {
        "foreground": "#5D6565",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["meta"],
      "settings": {
        "foreground": "#E2F6F5"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "meta.class",
        "meta.method",
        "storage",
        "variable",
        "meta.decorator"
      ],
      "settings": {
        "foreground": "#E7288E"
      }
    },
    {
      "scope": [
        "variable.object.property",
        "meta.function-call",
        "keyword.control.flow",
        "keyword.control.conditional",
        "keyword.control.loop",
        "keyword.control.switch",
        "keyword.control.trycatch",
        "keyword.control.return",
        "keyword.control.throw",
        "keyword.control.break",
        "keyword.control.continue",
        "keyword.control.goto",
        "keyword.control.else",
        "keyword.control.elseif",
        "keyword.control.default",
        "keyword.control.case",
        "keyword.control.finally",
        "keyword.control.export",
        "keyword.control.import",
        "keyword.control.from",
        "variable.language.this",
        "meta.import",
        "constant"
      ],
      "settings": {
        "foreground": "#FFD725"
      }
    },
    {
      "scope": [
        "meta.block",
        "string",
        "punctuation.definition.block",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#4FD4CB"
      }
    },
    {
      "scope": [
        "meta.parameters",
        "variable.parameter",
        "variable.other.object.property",
        "variable.other.property",
        "punctuation.separator",
        "punctuation.accessor",
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#E2F6F5"
      }
    },

    {
      "scope": ["meta.function-call", "meta.parameters", "string"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "meta.decorator",
        "entity.name.function",
        "keyword.control",
        "meta.type"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["variable.other.object.property", "variable.other.object"],
      "settings": {
        "fontStyle": "none"
      }
    },
    //#endregion

    //#region <HTML>
    {
      "scope": ["entity.name.tag.html"],
      "settings": {
        "foreground": "#E7288E"
      }
    },
    {
      "scope": ["entity.other.attribute-name.html"],
      "settings": {
        "foreground": "#FFD725"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.html",
        "text.html.derivative",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "meta.tag.structure.div.start",
        "meta.tag.structure.div.end",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end"
      ],
      "settings": {
        "foreground": "#5D6565",
        "fontStyle": "italic"
      }
    },
    //#endregion

    //#region <CSS>
    {
      "scope": [
        "entity.other.attribute-name",
        "entity.name.tag.input",
        "entity.name.tag",
        "support.constant.media"
      ],
      "settings": {
        "foreground": "#E7288E"
      }
    },
    {
      "scope": ["support.function.misc"],
      "settings": {
        "foreground": "#FFD725"
      }
    },
    {
      "scope": [
        // "entity.other.attribute-name.id",
        "entity.other.attribute-name.pseudo-class",
        "variable.scss",
        "variable.css",
        "variable.less",
        "variable.sass",
        "variable.other",
        "keyword.control.at-rule.media"
      ],
      "settings": {
        "foreground": "#4FD4CB"
      }
    },
    {
      "scope": ["punctuation.definition.entity"],
      "settings": {
        "foreground": "#E2F6F5"
      }
    },
    //#endregion

    //#region <JS>
    {
      "scope": ["support.type.object"],
      "settings": {
        "foreground": "#E7288E"
      }
    },
    //#endregion

    //#region <JSON>

    {
      "scope": [
        "support.type.property-name",
        "support.type.property-name.json",
        "support.type.property-name.json.comments",
        "string.json.comments",
      ],
      "settings": {
        "foreground": "#E7288E"
      }
    },
    {
      "scope": [
        "support.type.property-name",
        "support.type.property-name.json",
        "support.type.property-name.json.comments",
        "string.json.comments",
      ],
      "settings": {
        "fontStyle": "none"
      }
    },
    //#endregion

    //#region <Markdown>

    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#E7288E"
      }
    },
    //#endregion

    //#region <Python>

    //#endregion

    //#region <Additional>
    {
      "scope": ["invalid", "invalid.illegal", "invalid.deprecated"],
      "settings": {
        "foreground": "#F93319"
      }
    }
    //#region
  ]
}
